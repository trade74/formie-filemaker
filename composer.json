{
    "name": "craftyfm/craft-formie-filemaker",
    "description": "Send form data from Formie to Filemaker Data API v2 via a Webhook",
    "type": "craft-plugin",
    "license": "proprietary",
    "support": {
        "email": "stuart@x2network.net"
    },
    "require": {
        "php": ">=8.0.2",
        "craftcms/cms": "^4.6.0"
    },
    "require-dev": {
        "craftcms/ecs": "dev-main",
        "craftcms/phpstan": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "craftyfm\\craftformiefilemaker\\": "src/"
        }
    },
    "extra": {
        "handle": "formie-filemaker",
        "name": "Formie Filemaker",
        "developer": "Craftyfm",
        "documentationUrl": "",
        "class": "craftyfm\\craftformiefilemaker\\FormieFilemaker"
    },
    "scripts": {
        "check-cs": "ecs check --ansi",
        "fix-cs": "ecs check --ansi --fix",
        "phpstan": "phpstan --memory-limit=1G"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.0.2"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}

